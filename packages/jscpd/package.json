{
  "name": "jscpd",
  "version": "3.5.3",
  "description": "detector of copy/paste in files",
  "author": "Andrey Kucherenko <kucherenko.andrey@gmail.com>",
  "homepage": "https://github.com/kucherenko/jscpd#readme",
  "license": "MIT",
  "main": "src/index",
  "types": "src/index",
  "bin": {
    "jscpd": "./bin/jscpd"
  },
  "directories": {
    "lib": "src",
    "bin": "bin",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kucherenko/jscpd.git"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepare": "ts-node ../../build-utils/publish-util.ts ./package.json dist/index",
    "postpublish": "ts-node ../../build-utils/publish-util.ts ./package.json src/index",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@jscpd/core": "^3.5.0",
    "@jscpd/finder": "^3.5.1",
    "@jscpd/html-reporter": "^3.5.3",
    "@jscpd/tokenizer": "^3.5.1",
    "colors": "1.4.0",
    "commander": "^5.0.0",
    "fs-extra": "^9.1.0",
    "gitignore-to-glob": "^0.3.0"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/fs-extra": "^8.1.0",
    "jscpd-badge-reporter": "^1.1.3",
    "ts-node": "^8.8.2"
  },
  "preferGlobal": true,
  "gitHead": "fa06f839d6c5335a1e9dbf0c7387710fcd42eb54"
}
