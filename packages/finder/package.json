{
  "name": "@jscpd/finder",
  "version": "3.3.19",
  "description": "detector of copy/paste in files",
  "author": "Andrey Kucherenko <kucherenko.andrey@gmail.com>",
  "homepage": "https://github.com/kucherenko/jscpd#readme",
  "license": "MIT",
  "main": "src/index",
  "types": "src/index",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "html"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kucherenko/jscpd.git"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepare": "ts-node ../../build-utils/publish-util.ts ./package.json dist/index",
    "postpublish": "ts-node ../../build-utils/publish-util.ts ./package.json src/index",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@jscpd/core": "^3.3.17",
    "@jscpd/tokenizer": "^3.3.17",
    "blamer": "^1.0.1",
    "bytes": "^3.1.0",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "markdown-table": "^2.0.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/fs-extra": "^8.1.0",
    "@types/pug": "^2.0.4",
    "ts-node": "^8.8.2"
  },
  "gitHead": "c1cb71e4ea9c254cb16866d15b358f93c799f76c"
}
