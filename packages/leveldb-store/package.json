{
  "name": "@jscpd/leveldb-store",
  "version": "4.0.1",
  "description": "leveldb store for jscpd",
  "author": "Andrey Kucherenko <kucherenko.andrey@gmail.com>",
  "homepage": "https://github.com/kucherenko/jscpd#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./README.md": "./README.md"
  },
  "scripts": {
    "build": "tsup-node --dts",
    "dev": "nodemon",
    "test:dev": "vitest",
    "typecheck": "tsc",
    "cleanup": "rimraf ./dist .turbo"
  },
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kucherenko/jscpd.git"
  },
  "bugs": {
    "url": "https://github.com/kucherenko/jscpd/issues"
  },
  "dependencies": {
    "@jscpd/core": "workspace:*",
    "@jscpd/tokenizer": "workspace:*",
    "fs-extra": "^11.2.0",
    "level": "^8.0.1",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@jscpd/tsconfig": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.1.0",
    "nodemon": "^3.1.4",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4"
  },
  "gitHead": "beaae8ee77d5c7f41d2a0c65187887bd0dc87148"
}
