{
  "name": "@jscpd-ai/ollama-service",
  "version": "1.0.0",
  "description": "Ollama integration for jscpd-ai - local AI-powered code analysis",
  "author": "AI-enhanced fork",
  "homepage": "https://github.com/moinsen-dev/jscpd2025#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./README.md": "./README.md"
  },
  "scripts": {
    "build": "tsup-node --dts",
    "dev": "nodemon",
    "test:dev": "vitest",
    "typecheck": "tsc",
    "cleanup": "rimraf ./dist .turbo"
  },
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/moinsen-dev/jscpd2025.git"
  },
  "bugs": {
    "url": "https://github.com/moinsen-dev/jscpd2025/issues"
  },
  "dependencies": {
    "@jscpd-ai/core": "workspace:*",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@jscpd-ai/tsconfig": "workspace:*",
    "@types/node": "^22.5.2",
    "@types/node-fetch": "^2.6.11",
    "nodemon": "^3.1.4",
    "tsup": "^8.2.4",
    "vitest": "^2.0.5"
  }
}
